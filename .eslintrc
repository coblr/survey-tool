{
  "parser": "babel-eslint",
  "plugins": ["react", "jasmine", "prettier"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jasmine": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "printWidth": 70,
        "jsxBracketSameLine": true
      }
    ]
  }
}
